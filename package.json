{
  "name": "limitless-garden",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "9.2.x"
  },
  "scripts": {
    "start": "node ./target/src/server.js",
    "start:watch": "watch 'npm start' target",
    "start:full": "rimraf node_modules && npm install && npm start",
    "jshint": "jshint ./target/src/**.js",
    "jscs": "jscs --preset=google **.js",
    "copyfiles": "copyfiles 'public/**' 'spec/**/*.json' 'src/**/*.ejs'  target/",
    "build": "npm run tsc && npm run copyfiles && ng build",
    "tsc": "tsc",
    "tsc:watch": "tsc -w",
    "test": "./test.sh",
    "test:watch": "watch 'npm run tsc && npm run test' src spec public",
    "coverage": "./node_modules/.bin/istanbul cover --include-all-sources jasmine-node spec"
  },
  "dependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.0.0",
    "body-parser": "1.16.0",
    "bootstrap": "3.3.7",
    "cookie-parser": "1.4.3",
    "core-js": "2.5.1",
    "cors": "2.8.1",
    "debug": "2.2.0",
    "ejs": "2.3.4",
    "express": "4.13.4",
    "express-session": "1.15.5",
    "font-awesome": "4.7.0",
    "http-status": "0.2.5",
    "jquery": "^3.2.1",
    "kerberos": "0.0.17",
    "less-middleware": "2.2.1",
    "mongodb": "2.2.31",
    "morgan": "1.6.1",
    "node-statsd": "0.1.1",
    "passport": "0.3.2",
    "passport-google-oauth": "0.2.0",
    "reflect-metadata": "0.1.8",
    "rxjs": "^5.5.0",
    "serve-favicon": "2.3.2",
    "systemjs": "0.19.41",
    "util": "0.10.3",
    "winston": "2.3.1",
    "zone.js": "0.8.17"
  },
  "devDependencies": {
    "@angular/cli": "1.5.0",
    "@angular/compiler-cli": "^5.0.0",
    "@types/core-js": "0.9.43",
    "@types/jasmine": "2.5.54",
    "@types/node": "8.0.26",
    "ajv": "^5.0.0",
    "angular2-template-loader": "0.6.2",
    "awesome-typescript-loader": "3.2.3",
    "copyfiles": "^1.2.0",
    "istanbul": "^0.4.3",
    "jasmine": "^2.5.2",
    "jasmine-console-reporter": "1.2.7",
    "jasmine-core": "2.5.2",
    "jasmine-node": "^1.14.5",
    "jscs": "3.0.7",
    "jshint": "^2.8.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-spec-reporter": "0.0.31",
    "node-mocks-http": "1.6.6",
    "rewire": "^3.0.1",
    "rimraf": "^2.6.2",
    "superagent": "^3.8.0",
    "supertest": "3.0.0",
    "typescript": "2.4.2",
    "watch": "1.0.2"
  }
}
