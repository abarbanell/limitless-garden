{
  "index": "/spa/index.html",
  "assetGroups": [{
    "name": "app",
    "installMode": "prefetch",
    "resources": {
      "files": [
        "/favicon.ico",
        "/spa/index.html"
      ],
      "versionedFiles": [
        "/spa/*.bundle.css",
        "/spa/*.bundle.js",
        "/spa/*.chunk.js"
      ]
    }
  }, {
    "name": "assets",
    "installMode": "lazy",
    "updateMode": "prefetch",
    "resources": {
      "files": [
        "/spa/assets/**"
      ]
    }
  }],
  "dataGroups": [
    {
      "name": "api-freshness", 
      "urls": [
        "/api/**"
      ],
      "cacheConfig": {
        "strategy": "freshness", 
        "maxSize": 100,
        "maxAge": "3d", 
        "timeout": "2s"
      }
    }
  ]
}